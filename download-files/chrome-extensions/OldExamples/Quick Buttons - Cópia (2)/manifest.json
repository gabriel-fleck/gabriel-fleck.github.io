{
  "manifest_version": 2,

  "name": "Quick Buttons",
  "description": "Set messages to copy to clipboard",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
   "activeTab"
   ]
}