{
  "manifest_version": 2,

  "name": "Clipboard Messages",
  "description": "Set messages to copy to clipboard",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
   "activeTab",
   "storage"
   ]
}